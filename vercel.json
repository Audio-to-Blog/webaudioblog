{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/public/index.html"
      }
    ]
  }
  